<?xml version = "1.0" encoding = "UTF-8"?>

<beans xmlns = "http://www.springframework.org/schema/beans"
   xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation = "http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

   <bean id="triangle" class="spring.beanfactory.lchawla.autowire.Triangle" autowire="byName">
<!--    <bean id="triangle" class="spring.beanfactory.lchawla.autowire.Triangle" autowire="byType"> -->
<!--    <bean id="triangle" class="spring.beanfactory.lchawla.autowire.Triangle" autowire="constructor"> -->

   </bean>
	<bean id="pointA" class="spring.beanfactory.lchawla.autowire.Point" >
		<property name="x" value="0" />
		<property name="y" value="0" />
	</bean>
	<bean id="pointB" class="spring.beanfactory.lchawla.autowire.Point" >
		<property name="x" value="-20" />
		<property name="y" value="0" />
	</bean>
	<bean id="pointC" class="spring.beanfactory.lchawla.autowire.Point" >
		<property name="x" value="20" />
		<property name="y" value="0" />
	</bean>
	
</beans>
	